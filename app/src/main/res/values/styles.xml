<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Text_Page_title" parent="MyText">
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:fontFamily">@font/nsqr_bold</item>
    </style>

    <style name="Text_Regular_18dp" parent="MyText">
        <item name="android:textSize">18dp</item>
        <item name="android:fontFamily">@font/nsqr_regular</item></style>

    <style name="Text_Bold_16dp" parent="MyText">
        <item name="android:textSize">16dp</item>
        <item name="android:fontFamily">@font/nsqr_bold</item>
    </style>


</resources>